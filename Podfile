# This Podfile mimics dependencies pinned using Cartage 
# to perform OSA scan with Checkmarx (which do not support Carthage)

platform :ios, '11.4'

swift_version = '5.0'

target 'OSA' do

  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Alamofire', '5.4.2' # outdated
  pod 'Differentiator', '5.0.0' # outdated
  pod 'FBSDKCoreKit', '9.0.1'
  pod 'FirebaseAnalytics', '~> 7.11'
  pod 'FirebaseRemoteConfig', '~> 7.11'
  pod 'KeychainSwift', '19.0.0' # current
  pod 'MDFInternationalization', '2.0.0' # current
  pod 'MDFTextAccessibility', '2.0.1' # current
  pod 'Mapbox-iOS-SDK', '6.3.0' # current
  pod 'MaterialComponents', '124.1.1' # latest is 124.2
  pod 'Polyline', '5.0.2' # current
  pod 'RxCocoa', '6.1.0' # latest is 6.2.0
  pod 'RxDataSources', '5.0.0'
  pod 'RxOptional', '5.0.2'
  pod 'RxRelay', '6.1.0'
  pod 'RxSwift', '6.1.0'
  pod 'RxSwiftExt', '6.0.1'
  pod 'SDWebImage', '5.11.0'
  pod 'SnapKit', '5.0.1'
  pod 'Swinject', '~> 2.7'
  pod 'lottie-ios', '3.2.1'
  pod 'Turf', '1.2.0' # latest is 2.0.0-alpha.3

end
