# This Podfile mimics dependencies pinned using Cartage 
# to perform OSA scan with Checkmarx (which do not support Carthage)

platform :ios, '12.4'

swift_version = '5.0'

target 'OSA' do

  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'zxcvbn-ios', '1.0.4'
  pod 'AuthenticationManager', '~> 0.1.0' # should be 0.3.0
  pod 'objective-zip', '1.0.5' # DEPRECATED
  pod 'SkyFloatingLabelTextField', '3.8.0' # old
  pod 'SVProgressHUD', '2.1.2'
  pod 'MSAL', '~> 1.1.7'
  pod 'Objection', '~> 1.6'  
  pod 'CBOR', '~> 0.2'
  pod 'FormatterKit', '~> 1.9'
  pod 'JOSESwift', '~> 2.4'
  pod 'ReactiveObjC', '~> 3.1'
  pod 'AFNetworking', '3.2.1'

end
