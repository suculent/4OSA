# This Podfile mimics dependencies pinned using Cartage 
# to perform OSA scan with Checkmarx (which do not support Carthage)

platform :ios, '15.1'

# not sure for latest job
swift_version = '5.0'

target 'OSA' do

  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'CombineExt', '~> 1.5'

  pod 'FirebaseCore', '~> 8.12'

  pod 'GoogleDataTransport', '~> 9.1'
  pod 'GoogleUtilities', '~> 7.7'

  pod 'MDFInternationalization', '~> 3.0'
  pod 'MDFTextAccessibility', '~> 2.0'

  pod 'MaterialComponents', '~> 124.2'
  pod 'Reusable', '~> 4.1'
  pod 'SnapKit', '~> 5.0'
  pod 'nanopb', '~> 2.30908'

end
