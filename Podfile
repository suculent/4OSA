# This Podfile mimics dependencies pinned using Cartage 
# to perform OSA scan with Checkmarx (which do not support Carthage)

platform :ios, '13.0'

target 'OSA' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for OSA
  pod 'Alamofire', '4.9.1'
  pod 'ReactiveKit', '3.17.4'

  pod 'FloatingPanel', :git => 'https://github.com/scenee/FloatingPanel.git', :commit => 'a9e46f0de6fd4da97ee76c79959529fefd8b924a'
  pod 'ImagePalette', :git => 'https://github.com/niorko/ImagePalette.git', :branch => 'carthage-0.2.0'

  pod "Bond", "7.8.1"
  pod "SnapKit", "~> 5.0"
  pod "Swinject", "~> 2.6"

  pod "SwiftSoup", "2.2"
  pod "Differ", "1.4.5"
  pod "AppCenter", "~> 4.0"
  pod "SwiftProtobuf", "~> 1.13"
  pod "Swinject", "~> 2.6"
  pod "SwinjectAutoregistration", "~> 2.7"

  pod 'OCMock', '~> 3.7.1'
  pod 'Google-Maps-iOS-Utils', '~> 3.4.1'
  pod 'ZIPFoundation', '~> 0.9.11'
  pod 'youtube-ios-player-helper', :git => 'https://github.com/youtube/youtube-ios-player-helper.git', :commit => '5882789964ab7876c6a91dc905669535c0a90f92'

end
